EventHandler<HTMLSelectElement|HTMLInputElement>= (e) => {\n    updateFormData({\n      ...formData,\n      [e.target.name]: (select) ? 'false' : 'true'\n    });\n    setSelect(!select)\n  };\n\n\n  // object map for check with hook\n  const formHandler= (e:React.FormEvent<HTMLFormElement>)=> {\n    e.preventDefault();\n    \n      dispatch(pushData({key:objKey , data:formData}))  \n    e.currentTarget.reset()\n  }\n\n  return (\n    <>\n      <form onSubmit={formHandler} style={{ width: '100%' }} className='d-flex flex-wrap justify-content-center  p-4 '>\n        <div className='d-flex flex-wrap justify-content-between' style={{ maxWidth: '700px' }}>\n          {\n            inputs.map((x: any, i: number) => <div className=\"m-1 col-5\" style={{ minWidth: '290px' }}>\n              <label htmlFor=\"exampleInputEmail1\" className=\"form-label\">{x.name.replaceAll('_', ' ').charAt(0).toUpperCase() + x.name.replaceAll('_', ' ').slice(1)}</label>\n              {(x.type === 'textarea') ?\n                <textarea required={x.required} name={x.name} onChange={handleChange} className=\"form-control\" id={x.name + i} placeholder={`Enter ${x.name}...`} aria-describedby={`about ${x.name} `}></textarea>\n                : (x.type === 'select') ?\n                  <select required={x.required} name={x.name} className=\"form-select\" onChange={handleChange} aria-label=\"Default select example\">\n                    {\n                      (x.data.length > 0) ?\n                        [{ Name: '' }, ...x.data].map((x: any) => <option value={x.Name}>{(x.Name == '') ? 'select any' : x.Name}</option>)\n                        : <option value=''>No product Available</option>\n                    }\n                  </select> : (x.type === 'radio') ?\n                  <div className=\"form-check form-switch\">\n                    <input name={x.name} key={formData.toString()} onChange={HandleCheck} className=\"form-check-input\" type=\"checkbox\" id=\"flexSwitchCheckChecked\" checked={select} />\n                    <label className=\"form-check-label\" htmlFor=\"flexSwitchCheckChecked\">{x.span.replaceAll('_', ' ')}</label>\n                  </div> :\n                  <input required={x.required} name={x.name} onChange={handleChange} type={x.type} className=\"form-control\" id={x.name + i} aria-describedby={`about ${x.name} `} placeholder={`Enter ${x.name}`} />\n              }\n              {/* <div id=\"emailHelp\" className=\"form-text\">We'll never share your email with anyone else.</div> */}\n            </div>\n            )}\n        </div>\n        <div className=\"col-12\" style={{ maxWidth: '700px' }}>\n          <button type=\"submit\" className=\"btn btn-primary my-3  \">Submit</button>\n        </div>\n      </form>\n    </>\n  )\n}\n\nexport default AddProduct","/home/cedcoss/mark01/A-React Work/01_firstReactApp/94_ecommerce/admin/src/Components/body/Pages/Dashboard.tsx",["182","183"],[],"/home/cedcoss/mark01/A-React Work/01_firstReactApp/94_ecommerce/admin/src/Components/body/Pages/ListProducts.tsx",["184"],[],"/home/cedcoss/mark01/A-React Work/01_firstReactApp/94_ecommerce/admin/src/Components/body/Pages/Products.tsx",[],[],"/home/cedcoss/mark01/A-React Work/01_firstReactApp/94_ecommerce/admin/src/Components/Hooks/useMediaQuery.tsx",[],[],"/home/cedcoss/mark01/A-React Work/01_firstReactApp/94_ecommerce/admin/src/Components/body/assets/MyDoughnutChart.tsx",[],[],"/home/cedcoss/mark01/A-React Work/01_firstReactApp/94_ecommerce/admin/src/Components/body/assets/MyRadarChart.tsx",[],[],"/home/cedcoss/mark01/A-React Work/01_firstReactApp/94_ecommerce/admin/src/Components/body/assets/MyLineChart.tsx",[],[],"/home/cedcoss/mark01/A-React Work/01_firstReactApp/94_ecommerce/admin/src/Components/body/Pages/DataTables.tsx",["185","186"],[],"/home/cedcoss/mark01/A-React Work/01_firstReactApp/94_ecommerce/admin/src/Features/Store.tsx",[],[],"/home/cedcoss/mark01/A-React Work/01_firstReactApp/94_ecommerce/admin/src/Features/Slice.